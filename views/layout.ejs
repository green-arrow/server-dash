    <script src="/js/ember-app/routes/authorized-base-route.js"></script>
    <script src="/js/dashboard.js"></script>
    <script src="/js/dashboard.packery.js"></script>
    <script src="/js/ember-app/adapters/application-adapter.js"></script>
    <script src="/js/ember-app/controllers/account-controller.js"></script>
    <script src="/js/ember-app/controllers/account-setup-controller.js"></script>
    <script src="/js/ember-app/controllers/active-profile-controller.js"></script>
    <script src="/js/ember-app/controllers/application-controller.js"></script>
    <script src="/js/ember-app/controllers/login-controller.js"></script>
    <script src="/js/ember-app/controllers/profiles-controller.js"></script>
    <script src="/js/ember-app/controllers/widgets/widgets-add-controller.js"></script>
    <script src="/js/ember-app/helpers/render-widget.js"></script>
    <script src="/js/ember-app/models/profile-model.js"></script>
    <script src="/js/ember-app/models/profile-widget-model.js"></script>
