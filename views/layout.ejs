    <script src="/js/ember-app/models/widget-model.js"></script>
    <script src="/js/ember-app/models/widgets/widget-general-model.js"></script>
    <script src="/js/ember-app/router.js"></script>
    <script src="/js/ember-app/routes/account-route.js"></script>
    <script src="/js/ember-app/routes/account-setup-route.js"></script>
    <script src="/js/ember-app/routes/active-profile-route.js"></script>
    <script src="/js/ember-app/routes/index-route.js"></script>
    <script src="/js/ember-app/routes/login-route.js"></script>
    <script src="/js/ember-app/routes/profiles-route.js"></script>
    <script src="/js/ember-app/routes/widgets/widgets-add-route.js"></script>
